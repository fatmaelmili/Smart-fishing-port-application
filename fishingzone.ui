   ======================= */
QFrame#formAddStaff {
    background-color: rgba(255,255,255,0.06);
    border: 1px solid rgba(79,195,247,0.18);
    border-radius: 16px;
}

/* Title label (labeladdstaff) */
QLabel#labeladdstaff {
    color: #E3F2FD;
    font-size: 18px;
    font-weight: 700;
    padding: 8px 0;
}

/* All labels inside the form */
QFrame#formAddStaff QLabel {
    color: rgba(227,242,253,0.88);
    font-size: 13px;
    font-weight: 600;
}

/* Inputs (LineEdits) */
QLineEdit {
    background-color: rgba(10, 35, 55, 0.65);
    color: #E3F2FD;
    border: 1px solid rgba(79,195,247,0.25);
    border-radius: 10px;
    padding: 8px 12px;
    min-height: 36px;
    font-size: 13px;
}

QLineEdit:focus {
    border: 1px solid #4FC3F7;
    background-color: rgba(10, 35, 55, 0.85);
}

/* ComboBoxes (role, cvstat) */
QComboBox {
    background-color: rgba(10, 35, 55, 0.65);
    color: #E3F2FD;
    border: 1px solid rgba(79,195,247,0.25);
    border-radius: 10px;
    padding: 7px 12px;
    min-height: 36px;
    font-size: 13px;
}

QComboBox:hover {
    border: 1px solid rgba(79,195,247,0.45);
}

QComboBox:focus {
    border: 1px solid #4FC3F7;
    background-color: rgba(10, 35, 55, 0.85);
}

QComboBox::drop-down {
    width: 36px;
    border: none;
}

QComboBox::down-arrow {
    image: none;
    border-left: 6px solid transparent;
    border-right: 6px solid transparent;
    border-top: 8px solid rgba(227,242,253,0.8);
    margin-right: 10px;
}

/* Upload CV Button (uploadcvbtn) */
QPushButton#uploadcvbtn {
    background-color: rgba(255,255,255,0.08);
    color: #E3F2FD;
    border: 1px dashed rgba(79,195,247,0.6);
    border-radius: 10px;
    padding: 8px 12px;
    font-weight: 600;
}

/* Hover Upload */
QPushButton#uploadcvbtn:hover {
    background-color: rgba(79,195,247,0.2);
    border-color: #4FC3F7;
}

/* Add Staff Button (addstaffbtn) */
QPushButton#addstaffbtn {
    background-color: #0288D1;
    color: white;
    border: none;
    border-radius: 12px;
    min-height: 40px;
    font-weight: bold;
}

QPushButton#addstaffbtn:hover {
    background-color: #039BE5;
}

QPushButton#addstaffbtn:pressed {
    background-color: #01579B;
}
