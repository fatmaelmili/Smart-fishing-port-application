        <string notr="true">/* =================================================
   PAGE STAFF MANAGEMENT - BACKGROUND
   ================================================= */
QWidget#pageStaffManagement {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:1,
        stop:0 #071a2c,
        stop:0.35 #082136,
        stop:0.7 #071c2d,
        stop:1 #040f1a);
    font-family: &quot;Segoe UI&quot;;
    color: #EAF0FF;
}

/* =================================================
   HEADER + SIDEBAR (SAME STYLE)
   ================================================= */
QFrame#header,
QFrame#sidebarMenu {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
        stop:0 rgba(12, 42, 75, 0.95),
        stop:0.55 rgba(8, 30, 55, 0.95),
        stop:1 rgba(6, 22, 40, 0.95));
    border: 1px solid rgba(255,255,255,0.12);
    border-radius: 16px;
}

QFrame#header {
    padding: 10px 16px;
}

QFrame#sidebarMenu {
    padding: 12px;
}

/* =================================================
   HEADER CONTENT
   ================================================= */
QLabel#titreHeader {
    color: #EAF0FF;
    font-size: 15px;
    font-weight: 800;
    letter-spacing: 0.5px;
}

QLabel#avatar {
    min-width: 42px;
    max-width: 42px;
    min-height: 42px;
    max-height: 42px;
    border-radius: 21px;
    border: 2px solid rgba(88,184,255,0.6);
    background-color: rgba(255,255,255,0.06);
}

/* User profile button */
QCommandLinkButton#userprofiledetails {
    background-color: rgba(8, 18, 38, 0.55);
    border: 1px solid rgba(255,255,255,0.15);
    border-radius: 12px;
    padding: 8px 14px;
    color: #EAF0FF;
    font-weight: 700;
}

QCommandLinkButton#userprofiledetails:hover {
    background-color: rgba(88,184,255,0.15);
    border: 1px solid rgba(88,184,255,0.45);
}

QCommandLinkButton#userprofiledetails:pressed {
    background-color: rgba(88,184,255,0.25);
}

/* =================================================
   SIDEBAR GROUP
   ================================================= */
QGroupBox#groupBox {
    border: none;
    margin-top: 6px;
}

/* =================================================
   SIDEBAR BUTTONS (BASE)
   ================================================= */
QPushButton#clientsmanagementBTN,
QPushButton#equipmentmanagementBTN,
QPushButton#fishingzonemanagementBTN,
QPushButton#staffmanagementBTN,
QPushButton#stockmanagementBTN {
    background: transparent;
    border: none;
    color: rgba(234,240,255,0.80);
    padding: 12px 14px;
    border-radius: 12px;
    text-align: left;
    font-size: 13px;
    font-weight: 650;
}

/* Hover effect */
QPushButton#clientsmanagementBTN:hover,
QPushButton#equipmentmanagementBTN:hover,
QPushButton#fishingzonemanagementBTN:hover,
QPushButton#staffmanagementBTN:hover,
QPushButton#stockmanagementBTN:hover {
    background-color: rgba(88,184,255,0.14);
    border: 1px solid rgba(88,184,255,0.25);
    color: #FFFFFF;
}

/* Pressed */
QPushButton#clientsmanagementBTN:pressed,
QPushButton#equipmentmanagementBTN:pressed,
QPushButton#fishingzonemanagementBTN:pressed,
QPushButton#staffmanagementBTN:pressed,
QPushButton#stockmanagementBTN:pressed {
    background-color: rgba(88,184,255,0.25);
}

/* =================================================
   ACTIVE / SELECTED BUTTON (Staff Management)
   ================================================= */
QPushButton[active=&quot;true&quot;] {
    background-color: rgba(10, 102, 255, 0.28);
    border: 1px solid rgba(10, 102, 255, 0.55);
    color: #FFFFFF;
    padding-left: 18px;
}
QPushButton#staffmanagementBTN[active=&quot;true&quot;] {
    background-color: rgba(10, 102, 255, 0.35);
    border-left: 4px solid #4FC3F7;
    color: white;
    font-weight: 600;
}

/* =================================================
   LOG OUT BUTTON
   ================================================= */
QPushButton#logOutBTN {
    background: transparent;
    border: none;
    color: rgba(234,240,255,0.65);
    padding: 12px 14px;
    border-radius: 12px;
    text-align: left;
    font-weight: 650;
}

QPushButton#logOutBTN:hover {
    background-color: rgba(255, 120, 120, 0.12);
    border: 1px solid rgba(255, 120, 120, 0.25);
    color: #FFC1C1;
}

/* =================================================
   MAIN CONTENT AREA
   ================================================= */
QFrame#contenumain {
    background-color: transparent;
}

/* =================================================
   MENUBAR + STATUSBAR
   ================================================= */
QMenuBar {
    background-color: rgba(6, 22, 40, 0.95);
    color: rgba(234,240,255,0.85);
    border-bottom: 1px solid rgba(255,255,255,0.12);
    padding: 6px;
}

QStatusBar {
    background-color: rgba(6, 22, 40, 0.95);
    color: rgba(234,240,255,0.70);
    border-top: 1px solid rgba(255,255,255,0.12);
}
/* =======================
   FORM CARD (LEFT)
   objectName: formAddStaff
   ======================= */
QFrame#formAddStaff {
    background-color: rgba(255,255,255,0.06);
    border: 1px solid rgba(79,195,247,0.18);
    border-radius: 16px;
}

/* Title label (labeladdstaff) */
QLabel#labeladdstaff {
    color: #E3F2FD;
    font-size: 18px;
    font-weight: 700;
    padding: 8px 0;
}

/* All labels inside the form */
QFrame#formAddStaff QLabel {
    color: rgba(227,242,253,0.88);
    font-size: 13px;
    font-weight: 600;
}

/* Inputs (LineEdits) */
QLineEdit {
    background-color: rgba(10, 35, 55, 0.65);
    color: #E3F2FD;
    border: 1px solid rgba(79,195,247,0.25);
    border-radius: 10px;
    padding: 8px 12px;
    min-height: 36px;
    font-size: 13px;
}

QLineEdit:focus {
    border: 1px solid #4FC3F7;
    background-color: rgba(10, 35, 55, 0.85);
}

/* ComboBoxes (role, cvstat) */
QComboBox {
    background-color: rgba(10, 35, 55, 0.65);
    color: #E3F2FD;
    border: 1px solid rgba(79,195,247,0.25);
    border-radius: 10px;
    padding: 7px 12px;
    min-height: 36px;
    font-size: 13px;
}

QComboBox:hover {
    border: 1px solid rgba(79,195,247,0.45);
}

QComboBox:focus {
    border: 1px solid #4FC3F7;
    background-color: rgba(10, 35, 55, 0.85);
}

QComboBox::drop-down {
    width: 36px;
    border: none;
}

QComboBox::down-arrow {
    image: none;
    border-left: 6px solid transparent;
    border-right: 6px solid transparent;
    border-top: 8px solid rgba(227,242,253,0.8);
    margin-right: 10px;
}

/* Upload CV Button (uploadcvbtn) */
QPushButton#uploadcvbtn {
    background-color: rgba(255,255,255,0.08);
    color: #E3F2FD;
    border: 1px dashed rgba(79,195,247,0.6);
    border-radius: 10px;
    padding: 8px 12px;
    font-weight: 600;
}

/* Hover Upload */
QPushButton#uploadcvbtn:hover {
    background-color: rgba(79,195,247,0.2);
    border-color: #4FC3F7;
}

/* Add Staff Button (addstaffbtn) */
QPushButton#addstaffbtn {
    background-color: #0288D1;
    color: white;
    border: none;
    border-radius: 12px;
    min-height: 40px;
    font-weight: bold;
}

QPushButton#addstaffbtn:hover {
    background-color: #039BE5;
}

QPushButton#addstaffbtn:pressed {
    background-color: #01579B;
}
/* ================================================= */
/* ============ STAFF LIST CONTAINER =============== */
/* ================================================= */

#liststaffff {
    background-color: rgba(12, 32, 52, 0.95);
    border: 1px solid rgba(79, 195, 247, 0.25);
    border-radius: 18px;
    padding: 15px;
    margin-left: 10px;
}

#liststaffff::title {
    subcontrol-origin: margin;
    left: 15px;
    top: 8px;
    padding: 0 8px;
    color: #4fc3f7;
    font-weight: 700;
    font-size: 14px;
}

/* ================================================= */
/* ================= SEARCH BAR ==================== */
/* ================================================= */

#staffsearchbarre {
    background-color: rgba(20, 45, 70, 0.9);
    border: 1px solid rgba(79, 195, 247, 0.4);
    border-radius: 14px;
    padding: 10px 15px;
    color: white;
    font-size: 13px;
}

#staffsearchbarre:focus {
    border: 1px solid #4fc3f7;
    background-color: rgba(30, 75, 120, 0.9);
}

/* ================================================= */
/* ================= SORT COMBO ==================== */
/* ================================================= */

#sortstaff {
    background-color: rgba(20, 45, 70, 0.9);
    border: 1px solid rgba(79, 195, 247, 0.4);
    border-radius: 12px;
    padding: 6px 12px;
    color: #e3f2fd;
}

#sortstaff:hover {
    background-color: rgba(79, 195, 247, 0.25);
}

/* ================================================= */
/* ================== TABLE ======================== */
/* ================================================= */

#tablestaff {
    background-color: rgba(10, 25, 40, 0.95);
    border: 1px solid rgba(79, 195, 247, 0.3);
    border-radius: 14px;
    gridline-color: rgba(79, 195, 247, 0.15);
    color: #e3f2fd;
    font-size: 12px;
}

#tablestaff::item {
    padding: 8px;
}

#tablestaff::item:selected {
    background-color: rgba(79, 195, 247, 0.35);
    color: white;
}

QHeaderView::section {
    background-color: rgba(15, 55, 90, 0.95);
    color: #4fc3f7;
    padding: 8px;
    border: none;
    font-weight: bold;
}

/* Scrollbars table */
QTableWidget QScrollBar:vertical {
    background: transparent;
    width: 8px;
}

QTableWidget QScrollBar::handle:vertical {
    background: #4fc3f7;
    border-radius: 4px;
}

QTableWidget QScrollBar::add-line,
QTableWidget QScrollBar::sub-line {
    height: 0;
}

/* ================================================= */
/* ================= BUTTONS ======================= */
/* ================================================= */


#cvanalysebtn,
#staticbtn,
#exportpdfstaffbtn {
    background-color: rgba(20, 60, 95, 0.9);
    border: 1px solid rgba(79, 195, 247, 0.4);
    border-radius: 12px;
    color: #e3f2fd;
    padding: 10px 16px;
    font-weight: 600;
}


#cvanalysebtn:hover,
#staticbtn:hover,
#exportpdfstaffbtn:hover {
    background-color: rgba(79, 195, 247, 0.35);
    color: white;
}

/* ===== DELETE BUTTON ===== */
#deletestaffbtn {
    background-color: rgba(183, 28, 28, 0.9);
    border: none;
    border-radius: 12px;
    color: white;
    padding: 10px 16px;
    font-weight: 600;
}

#deletestaffbtn:hover {
    background-color: #e53935;
    box-shadow: 0 0 15px rgba(244, 67, 54, 0.6);
}
/* ======================================= */
/* ========= MODIFY STAFF BUTTON ========= */
/* ======================================= */

#modifystaffbtn {
    background-color: #f9a825;            /* Jaune principal */
    color: #1a1a1a;
    border: none;
    border-radius: 14px;
    padding: 12px 20px;
    font-size: 14px;
    font-weight: 700;
}

/* Hover */
#modifystaffbtn:hover {
    background-color: #fbc02d;            /* Jaune plus clair */
    box-shadow: 0 0 15px rgba(251, 192, 45, 0.6);
}

/* Click (pressed) */
#modifystaffbtn:pressed {
    background-color: #f57f17;            /* Jaune fonc√© */
    box-shadow: inset 0 3px 6px rgba(0,0,0,0.35);
}
/* =========================================================
   BACK TO WELCOME BUTTON
   ========================================================= */
QPushButton#backWbtn {
    background: rgba(20, 38, 58, 0.65);   /* navy soft */
    color: rgba(234,240,255,0.90);
    border-radius: 14px;
    padding: 10px 18px;
    border: 1px solid rgba(120,160,200,0.25);
    font-size: 13px;
    font-weight: 700;
}

/* Hover */
QPushButton#backWbtn:hover {
    background: rgba(37, 99, 235, 0.20);  /* blue highlight */
    border: 1px solid rgba(88,184,255,0.55);
    color: #FFFFFF;
}

/* Pressed */
QPushButton#backWbtn:pressed {
    background: rgba(15, 23, 42, 0.95);
    border: 1px solid rgba(88,184,255,0.75);
}

/* Optional: disabled */
QPushButton#backWbtn:disabled {
    background: rgba(20, 38, 58, 0.35);
    color: rgba(234,240,255,0.45);
    border: 1px solid rgba(120,160,200,0.15);
}
