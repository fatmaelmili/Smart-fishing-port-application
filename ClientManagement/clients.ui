       <property name="styleSheet">
        <string notr="true">/* =========================================================
   STAFF DASHBOARD - GLOBAL
   ========================================================= */
QWidget#staffdash{
    background: qlineargradient(x1:0, y1:0, x2:1, y2:1,
        stop:0 #071a2c,
        stop:0.35 #082136,
        stop:0.7 #071c2d,
        stop:1 #040f1a);
    font-family: &quot;Segoe UI&quot;;
    color: #EAF0FF;
}

/* =========================================================
   HEADER + SIDEBAR (SAME STYLE)
   ========================================================= */
QFrame#header_D,
QFrame#sidebarMenu_D {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
        stop:0 rgba(12, 42, 75, 0.95),
        stop:0.55 rgba(8, 30, 55, 0.95),
        stop:1 rgba(6, 22, 40, 0.95));
    border: 1px solid rgba(255,255,255,0.12);
    border-radius: 16px;
}

QFrame#header_D {
    padding: 10px 16px;
}

QFrame#sidebarMenu_D {
    padding: 12px;
}

/* =========================================================
   HEADER CONTENT
   ========================================================= */
QLabel#titreHeader_D {
    color: #EAF0FF;
    font-size: 15px;
    font-weight: 800;
    letter-spacing: 0.5px;
}

QLabel#avatar_D {
    min-width: 42px;
    max-width: 42px;
    min-height: 42px;
    max-height: 42px;
    border-radius: 21px;
    border: 2px solid rgba(88,184,255,0.6);
    background-color: rgba(255,255,255,0.06);
}

/* User profile button */
QCommandLinkButton#userprofiledetails_D {
    background-color: rgba(8, 18, 38, 0.55);
    border: 1px solid rgba(255,255,255,0.15);
    border-radius: 12px;
    padding: 8px 14px;
    color: #EAF0FF;
    font-weight: 700;
}

QCommandLinkButton#userprofiledetails_D:hover {
    background-color: rgba(88,184,255,0.15);
    border: 1px solid rgba(88,184,255,0.45);
}

QCommandLinkButton#userprofiledetails_D:pressed {
    background-color: rgba(88,184,255,0.25);
}

/* =========================================================
   SIDEBAR GROUP
   ========================================================= */
QGroupBox#groupBox_D {
    border: none;
    margin-top: 6px;
}

/* =========================================================
   SIDEBAR BUTTONS (BASE)
   ========================================================= */
QPushButton#clientsmanagementBTN_D,
QPushButton#equipmentmanagementBTN_D,
QPushButton#fishingzonemanagementBTN_D,
QPushButton#staffmanagementBTN_D,
QPushButton#stockmanagementBTN_D {
    background: transparent;
    border: none;
    color: rgba(234,240,255,0.80);
    padding: 12px 14px;
    border-radius: 12px;
    text-align: left;
    font-size: 13px;
    font-weight: 650;
}

/* Hover effect */
QPushButton#clientsmanagementBTN_D:hover,
QPushButton#equipmentmanagementBTN_D:hover,
QPushButton#fishingzonemanagementBTN_D:hover,
QPushButton#staffmanagementBTN_D:hover,
QPushButton#stockmanagementBTN_D:hover {
    background-color: rgba(88,184,255,0.14);
    border: 1px solid rgba(88,184,255,0.25);
    color: #FFFFFF;
}

/* Pressed */
QPushButton#clientsmanagementBTN_D:pressed,
QPushButton#equipmentmanagementBTN_D:pressed,
QPushButton#fishingzonemanagementBTN_D:pressed,
QPushButton#staffmanagementBTN_D:pressed,
QPushButton#stockmanagementBTN_D:pressed {
    background-color: rgba(88,184,255,0.25);
}

/* =========================================================
   ACTIVE / SELECTED BUTTON (Staff Management)
   ========================================================= */
QPushButton[active=&quot;true&quot;] {
    background-color: rgba(10, 102, 255, 0.28);
    border: 1px solid rgba(10, 102, 255, 0.55);
    color: #FFFFFF;
    padding-left: 18px;
}

QPushButton#staffmanagementBTN_D[active=&quot;true&quot;] {
    background-color: rgba(10, 102, 255, 0.35);
    border-left: 4px solid #4FC3F7;
    color: white;
    font-weight: 600;
}

/* =========================================================
   LOG OUT BUTTON
   ========================================================= */
QPushButton#logOutBTN_D {
    background: transparent;
    border: none;
    color: rgba(234,240,255,0.65);
    padding: 12px 14px;
    border-radius: 12px;
    text-align: left;
    font-weight: 650;
}

QPushButton#logOutBTN_D:hover {
    background-color: rgba(255, 120, 120, 0.12);
    border: 1px solid rgba(255, 120, 120, 0.25);
    color: #FFC1C1;
}

/* =========================================================
   STAFF DASHBOARD - MAIN CONTAINER
   ========================================================= */
QFrame#contenumain_D {
    background: transparent;
}

/* Main page title */
QLabel#dashtitle {
    color: #EAF2FF;
    font-size: 18px;
    font-weight: 800;
    letter-spacing: 0.3px;
}

/* Small labels in general */
QFrame#contenumain_D QLabel {
    background: transparent;
}

/* =========================================================
   KPI CARD (Total Staff) - cardstaff
   ========================================================= */
QFrame#cardstaff {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:1,
        stop:0 #123A6B,
        stop:0.6 #0B2545,
        stop:1 #071A2E);
    border-radius: 18px;
    padding: 16px;
    border: 1px solid rgba(255,255,255,0.12);
}

QFrame#cardstaff:hover {
    border: 1px solid rgba(59,130,246,0.55);
    background: qlineargradient(x1:0, y1:0, x2:1, y2:1,
        stop:0 #174A8A,
        stop:0.6 #0E2F59,
        stop:1 #071A2E);
}

/* Text inside KPI */
QLabel#totalemp {
    color: rgba(255,255,255,0.85);
    font-size: 13px;
    font-weight: 700;
}

QLabel#numberemp {
    color: #FFFFFF;
    font-size: 32px;
    font-weight: 900;
}

/* =========================================================
   BEST EMPLOYEE PANEL - beststaff
   ========================================================= */
QFrame#beststaff {
    border-radius: 18px;
    padding: 16px;
    border: 1px solid rgba(255,255,255,0.10);
    background: rgba(3, 14, 26, 0.55);
}

QFrame#beststaff:hover {
    border: 1px solid rgba(255,255,255,0.18);
    background: rgba(3, 14, 26, 0.62);
}

/* Best employee title */
QLabel#tittlebest {
    color: #EAF2FF;
    font-size: 14px;
    font-weight: 900;
}

/* Best employee fields */
QLabel#namebest, QLabel#rolebest {
    color: rgba(234,242,255,0.90);
    font-size: 12.5px;
    font-weight: 650;
}

QLabel#rewardbest {
    color: rgba(34,197,94,0.95);
    font-size: 12.5px;
    font-weight: 900;
}

QLabel#trophy {
    background: transparent;
}

/* =========================================================
   CHART PANELS (statRole + statcvstatus)
   ========================================================= */
QGroupBox#statRole,
QGroupBox#statcvstatus {
    background: rgba(3, 14, 26, 0.55);
    border: 1px solid rgba(255,255,255,0.10);
    border-radius: 18px;
    margin-top: 18px;
    padding: 12px;
}

QGroupBox#statRole::title,
QGroupBox#statcvstatus::title {
    subcontrol-origin: margin;
    subcontrol-position: top center;
    padding: 0 10px;
    color: rgba(255,255,255,0.90);
    font-size: 12.5px;
    font-weight: 900;
}

/* =========================================================
   BUTTONS (export / back / browse) - COMMON BASE
   ========================================================= */
QPushButton#backsbtn,
QPushButton#exportbtn,
QPushButton#browse,
QPushButton#backstfbtn {
    color: rgba(255,255,255,0.92);
    border-radius: 12px;
    padding: 9px 16px;
    font-size: 13px;
    font-weight: 750;
    border: 1px solid rgba(255,255,255,0.12);
}

/* ======================
   BACK BUTTON
   ====================== */
QPushButton#backsbtn,
QPushButton#backstfbtn {
    background: rgba(30, 41, 59, 0.65);
    border: 1px solid rgba(148,163,184,0.25);
}

QPushButton#backsbtn:hover,
QPushButton#backstfbtn:hover {
    background: rgba(51, 65, 85, 0.85);
    border: 1px solid rgba(148,163,184,0.45);
}

QPushButton#backsbtn:pressed,
QPushButton#backstfbtn:pressed {
    background: rgba(15, 23, 42, 0.92);
}

/* ======================
   EXPORT BUTTON
   ====================== */
QPushButton#exportbtn {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
        stop:0 #0F766E,
        stop:1 #064E4A);
    border: 1px solid rgba(34,197,94,0.45);
    font-size: 14px;
    font-weight: 850;
    border-radius: 14px;
    padding: 10px 20px;
}

QPushButton#exportbtn:hover {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
        stop:0 #14B8A6,
        stop:1 #0F766E);
    border: 1px solid rgba(34,197,94,0.75);
}

QPushButton#exportbtn:pressed {
    background: #0F766E;
}

/* ======================
   BROWSE BUTTON (upload image)
   ====================== */
QPushButton#browse {
    background: rgba(34, 197, 94, 0.12);
    border: 1px solid rgba(34, 197, 94, 0.35);
}

QPushButton#browse:hover {
    background: rgba(34, 197, 94, 0.18);
    border: 1px solid rgba(34, 197, 94, 0.55);
}

QPushButton#browse:pressed {
    background: rgba(34, 197, 94, 0.25);
}

/* =========================================================
   QLineEdit (if any)
   ========================================================= */
QLineEdit {
    background-color: rgba(13, 27, 42, 0.85);
    color: white;
    border: 1px solid rgba(59,130,246,0.35);
    border-radius: 10px;
    padding: 8px 10px;
    font-size: 13px;
}

QLineEdit:focus {
    border: 1px solid rgba(59,130,246,0.65);
}
