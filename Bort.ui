    border-radius: 12px;
    text-align: left;
    font-size: 13px;
    font-weight: 650;
}

/* Hover effect */
QPushButton#staffclientsBTN_DC:hover,
QPushButton#equipmentmanagementBTN_DC:hover,
QPushButton#fishingzonemanagementBTN_DC:hover,
QPushButton#staffmanagementBTN_DC:hover,
QPushButton#stockmanagementBTN_DC:hover {
    background-color: rgba(88,184,255,0.14);
    border: 1px solid rgba(88,184,255,0.25);
    color: #FFFFFF;
}

/* Pressed */
QPushButton#staffclientsBTN_DC:pressed,
QPushButton#equipmentmanagementBTN_DC:pressed,
QPushButton#fishingzonemanagementBTN_DC:pressed,
QPushButton#staffmanagementBTN_DC:pressed,
QPushButton#stockmanagementBTN_DC:pressed {
    background-color: rgba(88,184,255,0.25);
}

/* =========================================================
   ACTIVE / SELECTED BUTTON (Staff Management)
   ========================================================= */
QPushButton[active=&quot;true&quot;] {
    background-color: rgba(10, 102, 255, 0.28);
