     <property name="styleSheet">
      <string notr="true">/* =================================================
   PAGE STAFF MANAGEMENT - BACKGROUND
   ================================================= */
QWidget#pageupdate {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:1,
        stop:0 #071a2c,
        stop:0.35 #082136,
        stop:0.7 #071c2d,
        stop:1 #040f1a);
    font-family: &quot;Segoe UI&quot;;
    color: #EAF0FF;
}

/* =================================================
   HEADER + SIDEBAR (SAME STYLE)
   ================================================= */
QFrame#header_U,
QFrame#sidebarMenu_U {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
        stop:0 rgba(12, 42, 75, 0.95),
        stop:0.55 rgba(8, 30, 55, 0.95),
        stop:1 rgba(6, 22, 40, 0.95));
    border: 1px solid rgba(255,255,255,0.12);
    border-radius: 16px;
}

QFrame#header_U{
    padding: 10px 16px;
}

QFrame#sidebarMenu_U {
    padding: 12px;
}

/* =================================================
   HEADER CONTENT
   ================================================= */
QLabel#titreHeader_U {
    color: #EAF0FF;
    font-size: 15px;
    font-weight: 800;
    letter-spacing: 0.5px;
}

QLabel#avatar_U {
    min-width: 42px;
    max-width: 42px;
    min-height: 42px;
    max-height: 42px;
    border-radius: 21px;
    border: 2px solid rgba(88,184,255,0.6);
    background-color: rgba(255,255,255,0.06);
}

/* User profile button */
QCommandLinkButton#userprofiledetails_U {
    background-color: rgba(8, 18, 38, 0.55);
    border: 1px solid rgba(255,255,255,0.15);
    border-radius: 12px;
    padding: 8px 14px;
    color: #EAF0FF;
    font-weight: 700;
}

QCommandLinkButton#userprofiledetails_U:hover {
    background-color: rgba(88,184,255,0.15);
    border: 1px solid rgba(88,184,255,0.45);
}

QCommandLinkButton#userprofiledetails_U:pressed {
    background-color: rgba(88,184,255,0.25);
}

/* =================================================
   SIDEBAR GROUP
   ================================================= */
QGroupBox#groupBox_U {
    border: none;
    margin-top: 6px;
}

/* =================================================
   SIDEBAR BUTTONS (BASE)
   ================================================= */
QPushButton#clientsmanagementBTN_U,
QPushButton#equipmentmanagementBTN_U,
QPushButton#fishingzonemanagementBTN_U,
QPushButton#staffmanagementBTN_U,
QPushButton#stockmanagementBTN_U {
    background: transparent;
    border: none;
    color: rgba(234,240,255,0.80);
    padding: 12px 14px;
    border-radius: 12px;
    text-align: left;
    font-size: 13px;
    font-weight: 650;
}

/* Hover effect */
QPushButton#clientsmanagementBTN_U:hover,
QPushButton#equipmentmanagementBTN_U:hover,
QPushButton#fishingzonemanagementBTN_U:hover,
QPushButton#staffmanagementBTN_U:hover,
QPushButton#stockmanagementBTN_U:hover {
    background-color: rgba(88,184,255,0.14);
    border: 1px solid rgba(88,184,255,0.25);
    color: #FFFFFF;
}

/* Pressed */
QPushButton#clientsmanagementBTN_U:pressed,
QPushButton#equipmentmanagementBTN_U:pressed,
QPushButton#fishingzonemanagementBTN_U:pressed,
QPushButton#staffmanagementBTN_U:pressed,
QPushButton#stockmanagementBTN_U:pressed {
    background-color: rgba(88,184,255,0.25);
}

/* =================================================
   ACTIVE / SELECTED BUTTON (Staff Management)
   ================================================= */
QPushButton[active=&quot;true&quot;] {
    background-color: rgba(10, 102, 255, 0.28);
    border: 1px solid rgba(10, 102, 255, 0.55);
    color: #FFFFFF;
    padding-left: 18px;
}
QPushButton#staffmanagementBTN_U[active=&quot;true&quot;] {
    background-color: rgba(10, 102, 255, 0.35);
    border-left: 4px solid #4FC3F7;
    color: white;
    font-weight: 600;
}

/* =================================================
   LOG OUT BUTTON
   ================================================= */
QPushButton#logOutBTN_U {
    background: transparent;
    border: none;
    color: rgba(234,240,255,0.65);
    padding: 12px 14px;
    border-radius: 12px;
    text-align: left;
    font-weight: 650;
}

QPushButton#logOutBTN_U:hover {
    background-color: rgba(255, 120, 120, 0.12);
    border: 1px solid rgba(255, 120, 120, 0.25);
    color: #FFC1C1;
}

/* =================================================
   MAIN CONTENT AREA
   ================================================= */
QFrame#contenumain_U {
    background-color: transparent;
}

/* =================================================
   MENUBAR + STATUSBAR
   ================================================= */
QMenuBar_U {
    background-color: rgba(6, 22, 40, 0.95);
    color: rgba(234,240,255,0.85);
    border-bottom: 1px solid rgba(255,255,255,0.12);
    padding: 6px;
}

QStatusBar_U {
    background-color: rgba(6, 22, 40, 0.95);
    color: rgba(234,240,255,0.70);
    border-top: 1px solid rgba(255,255,255,0.12);
}
/* =======================
