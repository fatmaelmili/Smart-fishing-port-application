    background-color: rgba(88,184,255,0.25);
}

/* =========================================================
   SIDEBAR GROUP
   ========================================================= */
QGroupBox#groupBox_DC {
    border: none;
    margin-top: 6px;
}

/* =========================================================
   SIDEBAR BUTTONS (BASE)
   ========================================================= */
QPushButton#staffclientsBTN_DC,
QPushButton#equipmentmanagementBTN_DC,
