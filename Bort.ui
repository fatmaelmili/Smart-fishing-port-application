    border-radius: 16px;
}

/* Title label (labeladdstaff) */
QLabel#labeladdstaff_U {
    color: #E3F2FD;
    font-size: 18px;
    font-weight: 700;
    padding: 8px 0;
}

/* All labels inside the form */
QFrame#formAddStaff_U QLabel {
    color: rgba(227,242,253,0.88);
    font-size: 13px;
    font-weight: 600;
}

/* Inputs (LineEdits) */
QLineEdit {
    background-color: rgba(10, 35, 55, 0.65);
    color: #E3F2FD;
    border: 1px solid rgba(79,195,247,0.25);
    border-radius: 10px;
    padding: 8px 12px;
    min-height: 36px;
    font-size: 13px;
}

QLineEdit:focus {
    border: 1px solid #4FC3F7;
    background-color: rgba(10, 35, 55, 0.85);
}

/* ComboBoxes (role, cvstat) */
QComboBox {
    background-color: rgba(10, 35, 55, 0.65);
    color: #E3F2FD;
    border: 1px solid rgba(79,195,247,0.25);
    border-radius: 10px;
    padding: 7px 12px;
    min-height: 36px;
    font-size: 13px;
}

QComboBox:hover {
