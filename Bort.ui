   ========================================================= */
QFrame#header_DC,
QFrame#sidebarMenu_DC {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
        stop:0 rgba(12, 42, 75, 0.95),
        stop:0.55 rgba(8, 30, 55, 0.95),
        stop:1 rgba(6, 22, 40, 0.95));
    border: 1px solid rgba(255,255,255,0.12);
    border-radius: 16px;
}

QFrame#header_DC {
    padding: 10px 16px;
}

QFrame#sidebarMenu_DC {
    padding: 12px;
}

/* =========================================================
   HEADER CONTENT
   ========================================================= */
QLabel#titreHeader_DC {
    color: #EAF0FF;
    font-size: 15px;
    font-weight: 800;
    letter-spacing: 0.5px;
}

QLabel#avatar_DC {
    min-width: 42px;
    max-width: 42px;
    min-height: 42px;
    max-height: 42px;
    border-radius: 21px;
    border: 2px solid rgba(88,184,255,0.6);
    background-color: rgba(255,255,255,0.06);
}

/* User profile button */
QCommandLinkButton#userprofiledetails_DC {
    background-color: rgba(8, 18, 38, 0.55);
    border: 1px solid rgba(255,255,255,0.15);
    border-radius: 12px;
    padding: 8px 14px;
    color: #EAF0FF;
    font-weight: 700;
}

QCommandLinkButton#userprofiledetails_DC:hover {
    background-color: rgba(88,184,255,0.15);
    border: 1px solid rgba(88,184,255,0.45);
}

