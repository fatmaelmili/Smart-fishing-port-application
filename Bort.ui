    border-radius: 16px;
    padding: 14px;
    border: 1px solid rgba(255,255,255,0.10);
}

QFrame#cardEmployees:hover,
QFrame#cardStock:hover,
QFrame#cardClients:hover,
QFrame#cardEqui:hover,
QFrame#cardWeather:hover {
    border: 1px solid rgba(59,130,246,0.35);
}

/* Titles */
QLabel#totalstaff,
QLabel#stocktotaltitle,
QLabel#Clientstitle,
QLabel#equititle,
QLabel#weathertitle {
    color: rgba(255,255,255,0.86);
    font-size: 13px;
    font-weight: 700;
}

/* Values */
QLabel#numberstaff,
QLabel#stockkkk,
QLabel#clientssssssss,
QLabel#boatt,
QLabel#temp {
    color: #FFFFFF;
    font-size: 28px;
    font-weight: 900;
}

/* =========================
   NEW COLOR PALETTE (marine premium)
   - less saturated, more elegant
   ========================= */

/* Staff: Deep Ocean Blue */
QFrame#cardEmployees {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:1,
        stop:0 rgba(20, 58, 108, 0.95),
        stop:0.55 rgba(12, 35, 68, 0.95),
        stop:1 rgba(8, 24, 44, 0.90));
}

/* Stock: Sea Amber (soft) */
QFrame#cardStock {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:1,
        stop:0 rgba(176, 120, 36, 0.92),
        stop:0.55 rgba(120, 78, 22, 0.92),
        stop:1 rgba(38, 26, 14, 0.90));
}

/* Clients: Steel Indigo */
QFrame#cardClients {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:1,
        stop:0 rgba(73, 90, 143, 0.95),
        stop:0.55 rgba(42, 55, 102, 0.95),
        stop:1 rgba(18, 24, 52, 0.90));
}

/* Equipment: Harbor Green (muted) */
QFrame#cardEqui {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:1,
        stop:0 rgba(20, 108, 88, 0.95),
        stop:0.55 rgba(12, 66, 54, 0.95),
        stop:1 rgba(8, 34, 28, 0.90));
}

/* Weather: Cold Teal */
QFrame#cardWeather {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:1,
        stop:0 rgba(14, 108, 125, 0.95),
        stop:0.55 rgba(10, 64, 78, 0.95),
        stop:1 rgba(6, 28, 36, 0.90));
}

/* =========================
   Subtle highlight strip (like modern cards)
   ========================= */
QFrame#cardEmployees,
QFrame#cardStock,
QFrame#cardClients,
QFrame#cardEqui,
QFrame#cardWeather {
    border-top: 1px solid rgba(255,255,255,0.16);
    border-left: 1px solid rgba(255,255,255,0.10);
}
