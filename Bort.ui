      <string notr="true">/* =========================================================
   STAFF DASHBOARD - GLOBAL
   ========================================================= */
QWidget#staffdash{
    background: qlineargradient(x1:0, y1:0, x2:1, y2:1,
        stop:0 #071a2c,
        stop:0.35 #082136,
        stop:0.7 #071c2d,
        stop:1 #040f1a);
    font-family: &quot;Segoe UI&quot;;
    color: #EAF0FF;
}

/* =========================================================
   HEADER + SIDEBAR (SAME STYLE)
   ========================================================= */
QFrame#header_D,
QFrame#sidebarMenu_D {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
        stop:0 rgba(12, 42, 75, 0.95),
        stop:0.55 rgba(8, 30, 55, 0.95),
        stop:1 rgba(6, 22, 40, 0.95));
    border: 1px solid rgba(255,255,255,0.12);
    border-radius: 16px;
}

QFrame#header_D {
    padding: 10px 16px;
}

QFrame#sidebarMenu_D {
    padding: 12px;
}

/* =========================================================
   HEADER CONTENT
   ========================================================= */
QLabel#titreHeader_D {
    color: #EAF0FF;
    font-size: 15px;
    font-weight: 800;
    letter-spacing: 0.5px;
}

QLabel#avatar_D {
    min-width: 42px;
    max-width: 42px;
    min-height: 42px;
    max-height: 42px;
    border-radius: 21px;
    border: 2px solid rgba(88,184,255,0.6);
    background-color: rgba(255,255,255,0.06);
}

/* User profile button */
QCommandLinkButton#userprofiledetails_D {
    background-color: rgba(8, 18, 38, 0.55);
    border: 1px solid rgba(255,255,255,0.15);
    border-radius: 12px;
    padding: 8px 14px;
    color: #EAF0FF;
    font-weight: 700;
}

QCommandLinkButton#userprofiledetails_D:hover {
    background-color: rgba(88,184,255,0.15);
    border: 1px solid rgba(88,184,255,0.45);
}

QCommandLinkButton#userprofiledetails_D:pressed {
    background-color: rgba(88,184,255,0.25);
}

/* =========================================================
   SIDEBAR GROUP
   ========================================================= */
QGroupBox#groupBox_D {
    border: none;
    margin-top: 6px;
}

/* =========================================================
